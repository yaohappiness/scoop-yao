        "if (!(is_admin)) {",
        "    error \"Administrator rights are required to install $app.\"",
        "    exit 1",
        "}",
        "SCHTASKS /DELETE /TN RDPWUpdater /F"
    ],
    "installer": {
        "file": "RDPW_installer.exe",
        "keep": true
    },
